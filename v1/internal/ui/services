[
  ${
    range(env('CONSUL_SERVICE_COUNT', 1 + (location.search.index || 0))).map(
      function(item, i)
      {
        return `
          {
            "Name":"service-${i}",
            "Tags": [
                ${
                    range(env('CONSUL_TAG_COUNT', fake.random.number(10))).map(
                        function(item, i)
                        {
                            return `"${fake.hacker.noun()}"`;
                        }
                    )
                }
            ],
            "Nodes":[
              ${
                range(100).map(
                  function(item, i)
                  {
                    return `"node-${i}"`;
                  }
                )
              }
            ],
            "ChecksPassing":${fake.random.number(10)},
            "ChecksWarning":${fake.random.number(10)},
            "ChecksCritical":${fake.random.number(10)}
          }
        `;
      }
    )
  }
]
