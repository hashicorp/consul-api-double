[
  {
    "Name":"all-passing",
      "Nodes":["dc48921ff9cc"],
      "ChecksPassing":${fake.random.number(100)},
      "ChecksWarning":0,
      "ChecksCritical":0
  },
  {
    "Name":"all-warning",
      "Nodes":["dc48921ff9cc"],
      "ChecksPassing":0,
      "ChecksWarning":${fake.random.number(100)},
      "ChecksCritical":0
  },
  {
    "Name":"all-critical",
      "Nodes":["dc48921ff9cc"],
      "ChecksPassing":0,
      "ChecksWarning":0,
      "ChecksCritical":${fake.random.number(100)}
  },
  ${
    range(10000).map(
      function(item, i)
      {
        return `
          {
            "Name":"service-${i}",
            "Nodes":[
              ${
                range(100).map(
                  function(item, i)
                  {
                    return `"node-${i}"`;
                  }
                )
              }
            ],
            "ChecksPassing":${fake.random.number(10)},
            "ChecksWarning":${fake.random.number(10)},
            "ChecksCritical":${fake.random.number(10)}
          }
        `;
      }
    )
  }
]
