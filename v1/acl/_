${
  location.pathname.get(2) === 'policy' && http.method === 'POST' ? `
{
  "ID": "${fake.random.uuid()}-${new Date().getTime()}",
  "Name": "${body.Name || ''}",
  "Description": "${body.Description || ''}",
  "Datacenters": ${fake.helpers.randomize(['null', '["dc-1", "dc-2"]'])},
  "Rules": "${body.Rules || ''}"
}
` : location.pathname.get(2) === 'token' && http.method === 'POST' ?
  `
{
    "AccessorID":"${fake.random.uuid()}",
    "SecretID":"${fake.random.uuid()}",
    "Name": "${body.Name || ''}",
    "Policies": ["dc2-service-register", "global-service-read"],
    "CreateTime":"2017-08-23T22:47:14.695408057Z",
    "CreateIndex":7,
    "ModifyIndex":7
}
  ` : ``
}

